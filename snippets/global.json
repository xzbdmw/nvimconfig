{
	"Basic": {
		"prefix": "ba",
		"body": "T1=$1 T2=$2 T0=$0"
	},
	"Placeholders": {
		"prefix": "pl",
		"body": "T1=${1:aa}\nT2=${2:<$1>}"
	},
	"on": {
		"prefix": "on",
		"body": "func(cause error) {$0\\}"
	},
	"Choices": {
		"prefix": "ch",
		"body": "T1=${1|a,b|} T2=${2|c,d|}"
	},
	"Linked": {
		"prefix": "li",
		"body": "T1=$1\nT1=$1"
	},
	"Variables": {
		"prefix": "va",
		"body": "Runtime: $VIMRUNTIME\n"
	},
	"Complex": {
		"prefix": "co",
		"body": [
			"T1=${1:$RANDOM}",
			"T3=${3:$1_${2:$1}}",
			"T2=$2"
		]
	}
}

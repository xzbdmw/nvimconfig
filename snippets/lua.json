{
  "block": {
    "body": [
      "local n = 1",
      "for i = 1, 1000000000 do",
      "    n = n + 1",
      "end"
    ],
    "prefix": "block",
    "desc": "block the editor"
  },
  "fun": {
    "body": [
      "function ()\n\t$0\nend"
    ],
    "prefix": "fun"
  },
  "perf": {
    "body": [
      "local time = vim.uv.hrtime()",
      "Time(time, \"${0}\")"
    ],
    "prefix": "perf"
  },
  "setup": {
    "body": [
      "require(\"${1}\").setup({",
      "    ${0}",
      "})"
    ],
    "prefix": "setup"
  },
  "vim": {
    "body": [
      "vim.schedule(${1:fn})"
    ],
    "prefix": "sch"
  }
}
